{
  "name": "d1-prisma",
  "version": "0.1.2",
  "type": "module",
  "description": "Bridge CLI to manage Cloudflare D1 migrations with Prisma",
  "main": "./dist/index.js",
  "bin": {
    "d1-prisma": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build src/index.ts --format esm --clean --minify --target node --outdir ./dist",
    "dev": "bun src/index.ts --format esm --watch",
    "start": "node dist/index.js",
    "prepublishOnly": "bun run build",
    "tsc:check": "tsc -b ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hiutaky/d1-prisma.git"
  },
  "bugs": {
    "url": "https://github.com/Hiutaky/d1-prisma/issues"
  },
  "homepage": "https://github.com/Hiutaky/d1-prisma#readme",
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "jsonc-parse": "^2.0.0",
    "smol-toml": "^1.6.0",
    "tiny-parse-argv": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "bun": "^1.3.5"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
